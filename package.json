{
	"name": "receipt-app",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"workspaces": [
		"apps/*",
		"packages/*",
		"scripts",
		"testing/*"
	],
	"scripts": {
		"backend:test": "NODE_OPTIONS='--import tsx' vitest",
		"db:generate-types": "dotenv -c -- yarn workspace @ra/db db:generate-types",
		"db:migrate": "dotenv -c -- yarn workspace @ra/db db:migrate",
		"db:migrate:production": "dotenv -c production -- yarn workspace @ra/db db:migrate",
		"dev": "dotenv -c -- yarn workspace @ra/scripts dev",
		"format": "prettier --write .",
		"format:verify": "prettier --check .",
		"frontend:test": "yarn workspace @ra/playwright test",
		"ios:build": "yarn workspace @ra/mobile ios:update-version && yarn workspace @ra/mobile ios:build",
		"ios:publish": "yarn workspace @ra/mobile ios:publish",
		"lint": "eslint --max-warnings=0",
		"lint-staged": "lint-staged",
		"lint:fix": "eslint --max-warnings=0 --fix",
		"native:dev": "yarn workspace @ra/mobile dev",
		"typecheck": "tsc --noEmit",
		"web:build": "yarn workspace @ra/web build",
		"web:dev": "dotenv -c -- yarn workspace @ra/web dev",
		"web:start": "yarn workspace @ra/web start"
	},
	"pre-commit": [
		"typecheck",
		"lint-staged"
	],
	"lint-staged": {
		"*.{j,t}s{,x}": [
			"prettier --write",
			"eslint --max-warnings=0 --fix"
		]
	},
	"resolutions": {
		"@heroui/toast@npm:2.0.12": "patch:@heroui/toast@npm%3A2.0.12#~/patches/@heroui-toast-npm-2.0.12.patch",
		"@react-aria/utils": "^3.28.1",
		"@react-types/shared": "^3.28.0",
		"@tanstack/query-core@npm:5.80.6": "patch:@tanstack/query-core@npm%3A5.80.6#~/patches/@tanstack-query-core-npm-5.80.6.patch",
		"@tanstack/react-query@^5.80.6": "patch:@tanstack/react-query@npm%3A5.80.6#./patches/@tanstack-react-query-npm-5.80.6.patch",
		"h3@npm:1.13.0": "^1.13.0",
		"vinxi@npm:0.5.3": "patch:vinxi@npm%3A0.5.7#~/patches/vinxi-npm-0.5.7.patch",
		"vite@npm:6.1.4": "^6.0.0"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.80.6",
		"@tanstack/react-query-devtools": "^5.80.6",
		"@trpc/client": "^11.4.3",
		"@trpc/react-query": "^11.4.3",
		"@trpc/server": "^11.4.3",
		"@trpc/tanstack-react-query": "^11.4.3",
		"cookie": "^1.0.2",
		"i18next": "^25.2.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-i18next": "^15.5.3",
		"react-native": "^0.79.2",
		"react-native-web": "^0.20.0",
		"remeda": "^2.25.0",
		"tailwindcss": "^3.4.17",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@babel/core": "^7.23.5",
		"@eslint/js": "^9.27.0",
		"@stylistic/eslint-plugin": "^5.0.0",
		"@tanstack/eslint-plugin-router": "^1.125.0",
		"@types/eslint-plugin-jsx-a11y": "^6.10.0",
		"@types/eslint-plugin-tailwindcss": "^3.17.0",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.6",
		"@types/react-native": "^0.73.0",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"@vitest/coverage-v8": "^3.2.2",
		"alias-hq": "^6.2.1",
		"dotenv": "^16.0.0",
		"dotenv-cli": "^5.1.0",
		"eslint": "^9.27.0",
		"eslint-config-airbnb-extended": "^1.0.11",
		"eslint-config-prettier": "^10.1.5",
		"eslint-import-resolver-typescript": "^4.3.5",
		"eslint-plugin-import-x": "^4.12.2",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-n": "^17.18.0",
		"eslint-plugin-package-json": "^0.31.0",
		"eslint-plugin-playwright": "^2.2.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^6.0.0-rc.1",
		"eslint-plugin-tailwindcss": "^3.18.0",
		"eslint-plugin-vitest": "^0.5.4",
		"expo-router": "^3.4.7",
		"globals": "^15.8.0",
		"jiti": "^2.4.2",
		"jsonc-eslint-parser": "^2.4.0",
		"lint-staged": "^15.2.7",
		"pre-commit": "^1.2.2",
		"prettier": "^3.6.2",
		"prettier-plugin-packagejson": "^2.5.17",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"sharp": "^0.32.6",
		"testcontainers": "^10.11.0",
		"ts-node": "^10.9.1",
		"tsx": "^4.7.1",
		"turbo": "^1.2.2",
		"typescript": "5.8.3",
		"typescript-eslint": "^8.32.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.2"
	},
	"packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
