{
	"name": "receipt-app",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"workspaces": [
		"apps/*",
		"packages/*",
		"testing/*",
		"utils/*"
	],
	"scripts": {
		"backend:test": "NODE_OPTIONS='--import tsx' vitest",
		"db:generate-types": "kysely-codegen",
		"db:migrate": "yarn workspace @ra/db db:migrate",
		"dev": "yarn workspace @ra/scripts dev",
		"format": "yarn workspace @ra/format write",
		"format:prepare": "yarn workspace @ra/format collect-ignore-rules",
		"format:verify": "yarn workspace @ra/format check",
		"frontend:test": "yarn workspace @ra/playwright test",
		"install:immutable": "yarn --immutable",
		"lint": "yarn workspace @ra/lint lint",
		"lint:fix": "yarn workspace @ra/lint lint --fix",
		"native:dev": "yarn workspace @ra/mobile expo start",
		"native:doctor": "yarn workspace @ra/mobile expo-doctor",
		"native:prebuild": "yarn workspace @ra/mobile expo prebuild",
		"native:run:android": "yarn workspace @ra/mobile css:prepare && yarn workspace @ra/mobile expo run:android",
		"native:run:ios": "yarn workspace @ra/mobile css:prepare && yarn workspace @ra/mobile expo run:ios",
		"typecheck": "tsgo --noEmit",
		"web:build": "yarn workspace @ra/web build",
		"web:dev": "yarn workspace @ra/web dev",
		"web:start": "yarn workspace @ra/web start"
	},
	"pre-commit": [
		"install:immutable",
		"typecheck",
		"lint-staged"
	],
	"resolutions": {
		"@expo/cli@npm:55.0.6": "patch:@expo/cli@npm%3A55.0.6#~/patches/@expo-cli-npm-55.0.6.patch",
		"@heroui/theme@npm:2.4.22": "patch:@heroui/theme@npm%3A2.4.22#~/patches/@heroui-theme-npm-2.4.22.patch",
		"@heroui/toast@npm:2.0.16": "patch:@heroui/toast@npm%3A2.0.16#~/patches/@heroui-toast-npm-2.0.16.patch",
		"@internationalized/date": "^3.10.0",
		"@tanstack/query-core@npm:5.80.6": "patch:@tanstack/query-core@npm%3A5.80.6#~/patches/@tanstack-query-core-npm-5.80.6.patch",
		"@tanstack/react-query@catalog:": "patch:@tanstack/react-query@npm%3A5.80.6#./patches/@tanstack-react-query-npm-5.80.6.patch",
		"@tanstack/start-plugin-core@npm:1.131.49": "patch:@tanstack/start-plugin-core@npm%3A1.131.49#~/patches/@tanstack-start-plugin-core-npm-1.131.49.patch",
		"framer-motion@npm:^11.11.11": "patch:framer-motion@npm%3A11.11.11#~/patches/framer-motion-npm-11.11.11.patch",
		"lucide-react@npm:^0.562.0": "patch:lucide-react@npm%3A0.562.0#~/patches/lucide-react-npm-0.562.0.patch",
		"react-native-color-matrix-image-filters@npm:^8.0.2": "patch:react-native-color-matrix-image-filters@npm%3A8.0.2#~/patches/react-native-color-matrix-image-filters-npm-8.0.2.patch",
		"react-native-web@npm:^0.21.2": "patch:react-native-web@npm%3A0.21.2#~/patches/react-native-web-npm-0.21.2.patch",
		"uniwind@catalog:": "patch:uniwind@npm%3A1.2.2#~/patches/uniwind-npm-1.2.2.patch"
	},
	"dependencies": {
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-native": "0.83.1",
		"remeda": "^2.25.0"
	},
	"devDependencies": {
		"@types/node": "^24.1.0",
		"@types/react": "^19.2.11",
		"@types/react-dom": "^19.2.3",
		"@typescript/native-preview": "7.0.0-dev.20260203.1",
		"@vitest/coverage-v8": "^3.2.2",
		"eslint": "^9.39.2",
		"kysely-codegen": "^0.19.0",
		"lint-staged": "^16.2.7",
		"pre-commit": "^1.2.2",
		"prettier": "^3.7.4",
		"sharp": "catalog:",
		"tsx": "^4.21.0",
		"typescript": "6.0.0-dev.20260203",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	},
	"packageManager": "yarn@4.12.0+sha512.f45ab632439a67f8bc759bf32ead036a1f413287b9042726b7cc4818b7b49e14e9423ba49b18f9e06ea4941c1ad062385b1d8760a8d5091a1a31e5f6219afca8"
}
