{
	"name": "receipt-app",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"workspaces": [
		"apps/*",
		"lint",
		"packages/*",
		"scripts",
		"testing/*"
	],
	"scripts": {
		"backend:test": "NODE_OPTIONS='--import tsx' vitest",
		"db:generate-types": "kysely-codegen",
		"db:migrate": "yarn workspace @ra/db db:migrate",
		"dev": "yarn workspace @ra/scripts dev",
		"format": "prettier --write .",
		"format:verify": "prettier --check .",
		"frontend:test": "yarn workspace @ra/playwright test",
		"install:immutable": "yarn --immutable",
		"ios:build": "yarn workspace @ra/mobile ios:update-version && yarn workspace @ra/mobile ios:build",
		"ios:publish": "yarn workspace @ra/mobile ios:publish",
		"lint": "yarn workspace @ra/lint lint",
		"lint:fix": "yarn workspace @ra/lint lint --fix",
		"native:dev": "yarn workspace @ra/mobile dev",
		"typecheck": "tsc --noEmit",
		"web:build": "yarn workspace @ra/web build",
		"web:dev": "yarn workspace @ra/web dev",
		"web:start": "yarn workspace @ra/web start"
	},
	"pre-commit": [
		"install:immutable",
		"typecheck",
		"lint-staged"
	],
	"resolutions": {
		"@heroui/toast@npm:2.0.16": "patch:@heroui/toast@npm%3A2.0.16#~/patches/@heroui-toast-npm-2.0.16.patch",
		"@internationalized/date": "^3.10.0",
		"@tanstack/query-core@npm:5.80.6": "patch:@tanstack/query-core@npm%3A5.80.6#~/patches/@tanstack-query-core-npm-5.80.6.patch",
		"@tanstack/react-query@^5.80.6": "patch:@tanstack/react-query@npm%3A5.80.6#./patches/@tanstack-react-query-npm-5.80.6.patch",
		"@tanstack/start-plugin-core@npm:1.131.49": "patch:@tanstack/start-plugin-core@npm%3A1.131.49#~/patches/@tanstack-start-plugin-core-npm-1.131.49.patch",
		"framer-motion@npm:^11.11.11": "patch:framer-motion@npm%3A11.11.11#~/patches/framer-motion-npm-11.11.11.patch",
		"h3@npm:1.13.0": "^1.13.0",
		"lucide-react@npm:^0.562.0": "patch:lucide-react@npm%3A0.562.0#~/patches/lucide-react-npm-0.562.0.patch",
		"react-native-web@npm:^0.21.1": "patch:react-native-web@npm%3A0.21.1#~/patches/react-native-web-npm-0.21.1.patch",
		"vite@npm:6.1.4": "^6.0.0"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.80.6",
		"@tanstack/react-query-devtools": "^5.80.6",
		"@trpc/client": "^11.4.3",
		"@trpc/react-query": "^11.4.3",
		"@trpc/server": "^11.4.3",
		"@trpc/tanstack-react-query": "^11.4.3",
		"cookie": "^1.0.2",
		"i18next": "^25.2.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-i18next": "^15.5.3",
		"react-native": "^0.81.4",
		"react-native-reanimated": "^4.1.2",
		"react-native-safe-area-context": "^5.6.1",
		"react-native-web": "^0.21.1",
		"react-native-worklets": "^0.6.0",
		"remeda": "^2.25.0",
		"tailwindcss": "^4.1.14",
		"uniwind": "^1.2.2",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@babel/core": "^7.23.5",
		"@tailwindcss/vite": "^4.1.14",
		"@types/node": "^24.1.0",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.6",
		"@types/react-native": "^0.73.0",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"@typescript-eslint/parser": "^8.38.0",
		"@vitest/coverage-v8": "^3.2.2",
		"alias-hq": "^6.2.1",
		"eslint": "^9.32.0",
		"expo-router": "^3.4.7",
		"kysely-codegen": "^0.19.0",
		"lint-staged": "^16.2.7",
		"nvmrc": "^1.1.1",
		"postcss": "^8.5.6",
		"pre-commit": "^1.2.2",
		"prettier": "^3.6.2",
		"prettier-plugin-packagejson": "^2.5.17",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sharp": "^0.32.6",
		"testcontainers": "^10.11.0",
		"tsx": "^4.7.1",
		"turbo": "^1.2.2",
		"typescript": "5.8.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	},
	"packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
