{
	"ts-node": {
		"esm": true,
		"compilerOptions": {
			"module": "esnext"
		}
	},
	"compilerOptions": {
		"strict": true,
		"strictNullChecks": true,
		"noUncheckedIndexedAccess": true,
		"paths": {
			"app/*": ["./packages/app/*"],
			"next-app/*": ["./apps/next/src/*"]
		}
	},
	"extends": "expo/tsconfig.base"
}
